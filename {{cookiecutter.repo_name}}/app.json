{
  "name": "{{ cookiecutter.repo_name }}",
  "env":{
    "DJANGO_SETTINGS_MODULE":{
      "value": "{{ cookiecutter.package_name }}.settings"
    },
    "SECRET_KEY": {
      "generator": "secret"
    }
  },
  "addons":[
    "heroku-postgresql:hobby-dev"
  ],
  "buildpacks": [
    {% if cookiecutter.use_mo_static == "y" -%}
    {
      "url": "heroku/nodejs"
    },
    {%- endif %}
    {
      "url": "heroku/python"
    }
  ]
}
